{
    "manifest_version": 3,
    "name": "Katalon Header Injector",
    "version": "2.0",
    "description": "Adds custom headers to all requests",
    "permissions": [
      "declarativeNetRequest"
    ],
    "declarative_net_request": {
        "rule_resources": [
          {
            "id": "ruleset_1",
            "enabled": true,
            "path": "rules.json"
          }
        ]
      },    
    "host_permissions": [
      "<all_urls>"
    ]
  }